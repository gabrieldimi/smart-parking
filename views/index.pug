extends default

block content
	script(src='/client.js')
	div.container
		each val in [1, 3, 5]
			div.parking-spot-image(style=`background: url('/images/ParkingSpace.png') top center no-repeat; background-size: cover;`)
				div.sensorLeft.free(id= 'A' + val)
				div.sensorRight.free(id= 'A' + (val+1))
	div.path
		div.striped-border
		
	div.container
		each val in [1, 3, 5]
			div.parking-spot-image(style=`background: url('/images/ParkingSpace1.png') top center no-repeat; background-size: cover;`)
				div.sensorLeft.free(id= 'B' + val)
				div.sensorRight.free(id= 'B' + (val+1))
